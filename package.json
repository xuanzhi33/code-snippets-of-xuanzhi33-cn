{
  "name": "code-snippets-of-xuanzhi33-cn",
  "displayName": "Code Snippets of xuanzhi33.cn",
  "description": "Easily write web apps with the same styling as xuanzhi33.cn",
  "version": "2.2.0",
  "publisher": "xuanzhi33",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuanzhi33/code-snippets-of-xuanzhi33-cn"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onCommand:code-snippets-of-xuanzhi33-cn.openWebsite"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-snippets-of-xuanzhi33-cn.openWebsite",
        "title": "Open xuanzhi33.cn"
      }
    ],
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/html.json"
      },
      {
        "language": "css",
        "path": "./snippets/html.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.2.2",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  }
}
